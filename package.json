{
  "name": "fintech-secure-native",
  "version": "1.0.26",
  "description": "Secure native view for displaying sensitive card data",
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index.ts",
  "source": "src/index.ts",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "build": "bob build",
    "clean": "del-cli lib android/build ios/build",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "release": "release-it",
    "prepublishOnly": "bob build"
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "secure",
    "card",
    "fintech",
    "security",
    "native-view",
    "screenshot-protection"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JorgeRojas827/fintech-secure-native.git"
  },
  "author": "Jorge Luis Rojas Poma <jorgerojaspoma09@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JorgeRojas827/fintech-secure-native/issues"
  },
  "homepage": "https://github.com/JorgeRojas827/fintech-secure-native#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "dependencies": {
    "crypto-js": "^4.2.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "devDependencies": {
    "@react-native/eslint-config": "^0.81.0",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^20.0.0",
    "@types/react": "^19.1.0",
    "@types/react-native": "^0.73.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "del-cli": "^5.1.0",
    "eslint": "^8.0.0",
    "jest": "^29.7.0",
    "react": "^19.1.0",
    "react-native": "^0.81.0",
    "react-native-builder-bob": "^0.30.0",
    "release-it": "^17.6.0",
    "typescript": "^5.5.4"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}
